{
  "name": "vc-vue-elm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "cross-env ANALYZE=false vue-cli-service build",
    "analyze": "cross-env ANALYZE=true vue-cli-service build",
    "lint": "vue-cli-service lint",
    "preview": "start cmd /K \"browser-sync start -s dist -f dist\"",
    "publish": "cp -r dist/* gh-pages && git -C ./gh-pages add -A && git -C ./gh-pages commit -m 'publish' && git -C ./gh-pages push || echo nothing to push"
  },
  "dependencies": {
    "es6-promise": "^4.2.4",
    "isomorphic-fetch": "^2.2.1",
    "lodash-es": "^4.17.10",
    "ngeohash": "^0.6.0",
    "qs": "^6.5.2",
    "vee-validate": "^2.1.0-beta.9",
    "vue": "^2.5.17",
    "vue-lazyload": "^1.2.6",
    "vue-router": "^3.0.1",
    "vue-swipe": "^2.4.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "node-sass": "^4.9.3",
    "postcss-px2rem": "^0.3.0",
    "prerender-spa-plugin": "^3.4.0",
    "sass-loader": "^7.1.0",
    "style-resources-loader": "^1.2.1",
    "stylelint-config-recommended": "^2.1.0",
    "svg-sprite-loader": "^4.1.3",
    "vue-template-compiler": "^2.5.17",
    "webpack-bundle-analyzer": "^3.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "stylelint": {
    "extends": "stylelint-config-recommended",
    "rules": {
      "block-no-empty": null,
      "no-extra-semicolons": null
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-px2rem": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
